	number = {6},
	journaltitle = {Neuropathology and Applied Neurobiology},
	shortjournal = {Neuropathol. Appl. Neurobiol.},
	author = {Redondo-Castro, Elena and Romero, Rafael and Torres-Espín, Abel and Utrera, Juana and Duque, Daniel and Junyent, Fèlix and Auladell, Carme},
	date = {2014-10},
	pmid = {24033405},
	keywords = {Animals, Male, Mice, alcoholism, biogenic amines, Biogenic Monoamines, brain, Brain, Brain Chemistry, Choline O-Acetyltransferase, {DEDTC}, Ditiocarb, Dopamine, Norepinephrine, psychiatric alterations, Receptors, Dopamine D2, Serotonin, toxicity, Tyrosine 3-Monooxygenase}
